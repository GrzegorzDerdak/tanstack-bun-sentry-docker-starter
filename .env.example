PORT=3000

# Sentry — client & server DSN (public, safe to expose in client bundle)
VITE_SENTRY_DSN=

# Sentry — build-time only (source map uploads)
SENTRY_ORG=
SENTRY_PROJECT=
SENTRY_AUTH_TOKEN=

# Custom environment variable to determine the environment of the application
# Examples: production, staging, development
# Defaults to production
SENTRY_ENVIRONMENT=production
